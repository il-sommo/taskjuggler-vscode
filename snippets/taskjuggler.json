{
  "Project Definition": {
    "prefix": "project",
    "body": [
      "project ${1:project_id} \"${2:Project Name}\" ${3:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE} +${4:6}m {",
      "\ttimezone \"${5:UTC}\"",
      "\ttimeformat \"%Y-%m-%d\"",
      "\tnumberformat \"-\" \"\" \",\" \".\" 1",
      "\tcurrencyformat \"(\" \")\" \",\" \".\" 0",
      "\tcurrency \"${6:USD}\"",
      "\t",
      "\tscenario plan \"Plan\" {",
      "\t\tscenario actual \"Actual\"",
      "\t}",
      "\t$0",
      "}"
    ],
    "description": "Create a new TaskJuggler project (date: today)"
  },
  "Task Definition": {
    "prefix": "task",
    "body": [
      "task ${1:task_id} \"${2:Task Name}\" {",
      "\t$0",
      "}"
    ],
    "description": "Create a new task"
  },
  "Task with Effort": {
    "prefix": "task-effort",
    "body": [
      "task ${1:task_id} \"${2:Task Name}\" {",
      "\teffort ${3:5}d",
      "\tallocate ${4:resource}",
      "\t$0",
      "}"
    ],
    "description": "Create a task with effort and allocation"
  },
  "Task with Duration": {
    "prefix": "task-duration",
    "body": [
      "task ${1:task_id} \"${2:Task Name}\" {",
      "\tduration ${3:5}d",
      "\t$0",
      "}"
    ],
    "description": "Create a task with duration"
  },
  "Milestone": {
    "prefix": "milestone",
    "body": [
      "task ${1:milestone_id} \"${2:Milestone Name}\" {",
      "\tmilestone",
      "\tstart ${3:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE}",
      "\t$0",
      "}"
    ],
    "description": "Create a milestone task (date: today)"
  },
  "Resource Definition": {
    "prefix": "resource",
    "body": [
      "resource ${1:resource_id} \"${2:Resource Name}\" {",
      "\temail \"${3:email@example.com}\"",
      "\trate ${4:500.0}",
      "\t$0",
      "}"
    ],
    "description": "Create a new resource"
  },
  "Resource Team": {
    "prefix": "resource-team",
    "body": [
      "resource ${1:team_id} \"${2:Team Name}\" {",
      "\tresource ${3:member1} \"${4:Member 1}\" {",
      "\t\trate ${5:400.0}",
      "\t}",
      "\tresource ${6:member2} \"${7:Member 2}\" {",
      "\t\trate ${8:450.0}",
      "\t}",
      "\t$0",
      "}"
    ],
    "description": "Create a team of resources"
  },
  "Account Definition": {
    "prefix": "account",
    "body": [
      "account ${1:account_id} \"${2:Account Name}\" {",
      "\t$0",
      "}"
    ],
    "description": "Create an account"
  },
  "Account Structure": {
    "prefix": "account-full",
    "body": [
      "account cost \"Project Cost\" {",
      "\taccount dev \"Development\"",
      "\taccount doc \"Documentation\"",
      "\taccount test \"Testing\"",
      "}",
      "account rev \"Revenue\"",
      "balance cost rev",
      "$0"
    ],
    "description": "Create a complete account structure"
  },
  "Task Report": {
    "prefix": "taskreport",
    "body": [
      "taskreport ${1:report_id} \"${2:Report Title}\" {",
      "\tformats html",
      "\tcolumns bsi { title 'WBS' },",
      "\t        name,",
      "\t        start,",
      "\t        end,",
      "\t        effort { title \"Work\" },",
      "\t        chart { scale day width 500 }",
      "\t",
      "\tloadunit days",
      "\tcaption '${3:Report description}'",
      "\t$0",
      "}"
    ],
    "description": "Create a task report"
  },
  "Resource Report": {
    "prefix": "resourcereport",
    "body": [
      "resourcereport ${1:report_id} \"${2:Report Title}\" {",
      "\tformats html",
      "\tcolumns name,",
      "\t        effort { title \"Total Effort\" },",
      "\t        chart { scale day }",
      "\t",
      "\tloadunit days",
      "\t$0",
      "}"
    ],
    "description": "Create a resource report"
  },
  "Export CSV": {
    "prefix": "export-csv",
    "body": [
      "export ${1:export_id} {",
      "\tformats csv",
      "\tstart ${2:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE}",
      "\tend ${3:$CURRENT_YEAR-12-31}",
      "\t$0",
      "}"
    ],
    "description": "Export data to CSV"
  },
  "Scenario": {
    "prefix": "scenario",
    "body": [
      "scenario ${1:scenario_id} \"${2:Scenario Name}\" {",
      "\t$0",
      "}"
    ],
    "description": "Create a scenario"
  },
  "Macro Definition": {
    "prefix": "macro",
    "body": [
      "macro ${1:macro_name} [",
      "\t$0",
      "]"
    ],
    "description": "Create a macro"
  },
  "Allocate Resources": {
    "prefix": "allocate",
    "body": [
      "allocate ${1:resource_id}$0"
    ],
    "description": "Allocate a resource to a task"
  },
  "Dependencies": {
    "prefix": "depends",
    "body": [
      "depends ${1:task_id}$0"
    ],
    "description": "Add task dependencies"
  },
  "Shift Definition": {
    "prefix": "shift",
    "body": [
      "shift ${1:shift_id} \"${2:Shift Name}\" {",
      "\tworkinghours mon - fri 09:00 - 12:00, 13:00 - 18:00",
      "\tworkinghours sat, sun off",
      "\t$0",
      "}"
    ],
    "description": "Create a work shift"
  },
  "Leave Definition": {
    "prefix": "leave",
    "body": [
      "leaves ${1:holiday} \"${2:Holiday Name}\" ${3:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE}$0"
    ],
    "description": "Define a leave/holiday"
  },
  "Journal Entry": {
    "prefix": "journalentry",
    "body": [
      "journalentry ${1:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE} \"${2:Entry title}\" {",
      "\tauthor ${3:author_id}",
      "\tsummary \"${4:Summary}\"",
      "\tdetails -8<-",
      "\t${5:Detailed description}",
      "\t->8-",
      "\t$0",
      "}"
    ],
    "description": "Create a journal entry"
  },
  "Working Hours": {
    "prefix": "workinghours",
    "body": [
      "workinghours ${1:mon} - ${2:fri} ${3:09:00} - ${4:18:00}$0"
    ],
    "description": "Define working hours"
  },
  "Limits": {
    "prefix": "limits",
    "body": [
      "limits {",
      "\tdailymax ${1:8.0}h",
      "\tweeklymax ${2:40.0}h",
      "\t$0",
      "}"
    ],
    "description": "Define resource limits"
  },
  "Complete Task Structure": {
    "prefix": "task-full",
    "body": [
      "task ${1:task_id} \"${2:Task Name}\" {",
      "\tpriority ${3:500}",
      "\teffort ${4:5}d",
      "\tallocate ${5:resource}",
      "\tdepends ${6:!previous_task}",
      "\t",
      "\tnote \"${7:Note}\"",
      "\t",
      "\ttask ${8:subtask_id} \"${9:Subtask}\" {",
      "\t\teffort ${10:2}d",
      "\t}",
      "\t$0",
      "}"
    ],
    "description": "Create a complete task structure with subtasks"
  },
  "Text Report": {
    "prefix": "textreport",
    "body": [
      "textreport ${1:report_id} \"${2:Report Title}\" {",
      "\tformats html",
      "\theader -8<-",
      "\t\t== ${3:Report Header} ==",
      "\t\t$0",
      "\t->8-",
      "}"
    ],
    "description": "Create a text report"
  },
  "Navigator": {
    "prefix": "navigator",
    "body": [
      "navigator navbar {",
      "\thidereport 0",
      "}"
    ],
    "description": "Create a navigation bar"
  },
  "Flag Definition": {
    "prefix": "flags",
    "body": [
      "flags ${1:flag_name}$0"
    ],
    "description": "Define a flag"
  },
  "Copyright": {
    "prefix": "copyright",
    "body": [
      "copyright \"${1:Copyright notice}\"$0"
    ],
    "description": "Add copyright information"
  },
  "Email": {
    "prefix": "email",
    "body": [
      "email \"${1:email@example.com}\"$0"
    ],
    "description": "Add email address"
  },
  "Rate": {
    "prefix": "rate",
    "body": [
      "rate ${1:500.0}$0"
    ],
    "description": "Set rate"
  },
  "Efficiency": {
    "prefix": "efficiency",
    "body": [
      "efficiency ${1:1.0}$0"
    ],
    "description": "Set efficiency factor"
  },
  "Priority": {
    "prefix": "priority",
    "body": [
      "priority ${1:500}$0"
    ],
    "description": "Set priority"
  },
  "Complete": {
    "prefix": "complete",
    "body": [
      "complete ${1:50}$0"
    ],
    "description": "Set completion percentage"
  },
  "Start Date": {
    "prefix": "start",
    "body": [
      "start ${1:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE}$0"
    ],
    "description": "Set start date"
  },
  "End Date": {
    "prefix": "end",
    "body": [
      "end ${1:$CURRENT_YEAR-12-31}$0"
    ],
    "description": "Set end date"
  },
  "Booking": {
    "prefix": "booking",
    "body": [
      "booking ${1:resource_id} ${2:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE} +${3:5}d { ${4:overtime 2} }$0"
    ],
    "description": "Create a booking"
  },
  "Account Report": {
    "prefix": "accountreport",
    "body": [
      "accountreport ${1:report_id} \"${2:Account Report}\" {",
      "\tformats html",
      "\tcolumns name, total, ${3|cost,revenue,profit|}",
      "\tbalance ${4|cost,revenue|}",
      "\t$0",
      "}"
    ],
    "description": "Create an account report"
  },
  "Timesheet Report": {
    "prefix": "timesheetreport",
    "body": [
      "timesheetreport ${1:report_id} \"${2:Timesheet}\" {",
      "\tformats html",
      "\tresource ${3:resource_id}",
      "\tweekly ${4:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE}",
      "\t$0",
      "}"
    ],
    "description": "Create a timesheet report"
  },
  "Trace Report": {
    "prefix": "tracereport",
    "body": [
      "tracereport ${1:report_id} \"${2:Trace Report}\" {",
      "\tformats html",
      "\tsortresources nameup",
      "\t$0",
      "}"
    ],
    "description": "Create a trace report for debugging"
  },
  "iCal Report": {
    "prefix": "icalreport",
    "body": [
      "icalreport ${1:report_id} \"${2:Calendar Export}\" {",
      "\ttask ${3:task_id}",
      "\t$0",
      "}"
    ],
    "description": "Export to iCalendar format"
  },
  "XML Report": {
    "prefix": "xmlreport",
    "body": [
      "xmlreport ${1:report_id} \"${2:XML Export}\" {",
      "\t$0",
      "}"
    ],
    "description": "Export to XML format"
  },
  "Vacation": {
    "prefix": "vacation",
    "body": [
      "vacation ${1:$CURRENT_YEAR-08-01} - ${2:$CURRENT_YEAR-08-15}$0"
    ],
    "description": "Define vacation period for a resource"
  },
  "Managers": {
    "prefix": "managers",
    "body": [
      "managers ${1:manager_id}$0"
    ],
    "description": "Assign managers to a resource"
  },
  "Responsible": {
    "prefix": "responsible",
    "body": [
      "responsible ${1:resource_id}$0"
    ],
    "description": "Set responsible person for a task"
  },
  "Timezone": {
    "prefix": "timezone",
    "body": [
      "timezone \"${1:UTC}\"$0"
    ],
    "description": "Set timezone"
  },
  "Scheduling ASAP": {
    "prefix": "asap",
    "body": [
      "scheduling asap$0"
    ],
    "description": "Schedule task as soon as possible"
  },
  "Scheduling ALAP": {
    "prefix": "alap",
    "body": [
      "scheduling alap$0"
    ],
    "description": "Schedule task as late as possible"
  },
  "Select Filter": {
    "prefix": "select",
    "body": [
      "select ${1|isleaf(),istask(),ismilestone(),isactive()|}$0"
    ],
    "description": "Filter tasks/resources with logical expressions"
  },
  "Hide Task": {
    "prefix": "hidetask",
    "body": [
      "hidetask ${1|~isleaf(),~istask(),~ismilestone()|}$0"
    ],
    "description": "Hide tasks based on condition"
  },
  "Hide Resource": {
    "prefix": "hideresource",
    "body": [
      "hideresource ${1|~isleaf(),isdutyof(ProjectManager)|}$0"
    ],
    "description": "Hide resources based on condition"
  },
  "Columns with Status": {
    "prefix": "columns-status",
    "body": [
      "columns bsi { title 'WBS' },",
      "\t        name,",
      "\t        start,",
      "\t        end,",
      "\t        status,",
      "\t        complete,",
      "\t        chart { scale ${1|day,week,month|} }$0"
    ],
    "description": "Report columns with status tracking"
  },
  "Columns Financial": {
    "prefix": "columns-financial",
    "body": [
      "columns name,",
      "\t        cost,",
      "\t        revenue,",
      "\t        profit { cellcolor plan.profit < 0 \"#ff0000\" }$0"
    ],
    "description": "Financial report columns"
  },
  "Load Unit Hours": {
    "prefix": "loadunit-hours",
    "body": [
      "loadunit hours$0"
    ],
    "description": "Set load unit to hours"
  },
  "Load Unit Days": {
    "prefix": "loadunit-days",
    "body": [
      "loadunit days$0"
    ],
    "description": "Set load unit to days"
  },
  "Load Unit Weeks": {
    "prefix": "loadunit-weeks",
    "body": [
      "loadunit weeks$0"
    ],
    "description": "Set load unit to weeks"
  },
  "Charge": {
    "prefix": "charge",
    "body": [
      "charge ${1:account_id} ${2:100.0}$0"
    ],
    "description": "Charge amount to account"
  },
  "Alternative": {
    "prefix": "alternative",
    "body": [
      "alternative ${1:task_id}$0"
    ],
    "description": "Define alternative task"
  },
  "Purge": {
    "prefix": "purge",
    "body": [
      "purge ${1|task,resource,account|}$0"
    ],
    "description": "Purge element from project"
  },
  "Include": {
    "prefix": "include",
    "body": [
      "include \"${1:filename.tji}\"$0"
    ],
    "description": "Include external file"
  },
  "Supplement": {
    "prefix": "supplement",
    "body": [
      "supplement ${1|task,resource,account|} ${2:id} {",
      "\t$0",
      "}"
    ],
    "description": "Supplement existing element"
  },
  "Export Definition": {
    "prefix": "export",
    "body": [
      "export ${1:definition_id} {",
      "\t${2|task,resource,account|} ${3:element_id}",
      "\t$0",
      "}"
    ],
    "description": "Export definitions to included files"
  },
  "Fail Condition": {
    "prefix": "fail",
    "body": [
      "fail ${1|condition,alert|}$0"
    ],
    "description": "Define failure condition"
  },
  "Alert": {
    "prefix": "alert",
    "body": [
      "alert ${1|red,yellow,green|} \"${2:Alert message}\"$0"
    ],
    "description": "Add status alert"
  },
  "Status Note": {
    "prefix": "statusnote",
    "body": [
      "statusnote \"${1:Status update}\"$0"
    ],
    "description": "Add status note"
  },
  "Persistent": {
    "prefix": "persistent",
    "body": [
      "persistent$0"
    ],
    "description": "Mark allocation as persistent"
  },
  "Minstart Constraint": {
    "prefix": "minstart",
    "body": [
      "minstart ${1:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE}$0"
    ],
    "description": "Set minimum start date"
  },
  "Maxstart Constraint": {
    "prefix": "maxstart",
    "body": [
      "maxstart ${1:$CURRENT_YEAR-06-01}$0"
    ],
    "description": "Set maximum start date"
  },
  "Minend Constraint": {
    "prefix": "minend",
    "body": [
      "minend ${1:$CURRENT_YEAR-03-01}$0"
    ],
    "description": "Set minimum end date"
  },
  "Maxend Constraint": {
    "prefix": "maxend",
    "body": [
      "maxend ${1:$CURRENT_YEAR-12-31}$0"
    ],
    "description": "Set maximum end date"
  },
  "Extend Definition": {
    "prefix": "extend",
    "body": [
      "extend ${1|task,resource,account,project,report|} {",
      "\t${2|text,reference,date|} ${3:property_name} \"${4:Property Label}\"",
      "\t$0",
      "}"
    ],
    "description": "Extend elements with custom properties"
  },
  "Tracking Scenario": {
    "prefix": "trackingscenario",
    "body": [
      "trackingscenario ${1:actual}$0"
    ],
    "description": "Set scenario for progress tracking"
  },
  "Now Reference": {
    "prefix": "now",
    "body": [
      "now ${1:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE-${2:12}:${3:00}}$0"
    ],
    "description": "Set current time for reporting"
  },
  "Period": {
    "prefix": "period",
    "body": [
      "period ${1:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE} - ${2:$CURRENT_YEAR-12-31}$0"
    ],
    "description": "Set report period"
  },
  "Caption": {
    "prefix": "caption",
    "body": [
      "caption '${1:Report description}'$0"
    ],
    "description": "Add report caption"
  },
  "Sort Tasks": {
    "prefix": "sorttasks",
    "body": [
      "sorttasks ${1|nameup,namedown,startup,startdown,endup,enddown|}$0"
    ],
    "description": "Sort tasks in report"
  },
  "Sort Resources": {
    "prefix": "sortresources",
    "body": [
      "sortresources ${1|nameup,namedown,startup,startdown,endup,enddown|}$0"
    ],
    "description": "Sort resources in report"
  }
}
