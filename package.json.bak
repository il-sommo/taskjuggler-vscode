{
  "name": "taskjuggler-syntax",
  "displayName": "TaskJuggler Language Support",
  "description": "Complete language support for TaskJuggler: syntax highlighting, snippets, folding, and IntelliSense",
  "version": "0.0.1",
  "buildDate": "2026-01-28",
  "publisher": "fabrizio-vacca",
  "author": "Fabrizio Vacca <fabrizio.vacca@gmail.com>",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "icon": "images/icon.png",
  "contributes": {
    "languages": [
      {
        "id": "taskjuggler",
        "aliases": [
          "TaskJuggler",
          "tjp"
        ],
        "extensions": [
          ".tjp",
          ".tji"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/file-icon.svg",
          "dark": "./images/file-icon.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "taskjuggler",
        "scopeName": "source.taskjuggler",
        "path": "./syntaxes/taskjuggler.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "taskjuggler",
        "path": "./snippets/taskjuggler.json"
      }
    ]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/il-sommo/taskjuggler-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/il-sommo/taskjuggler-vscode/issues"
  },
  "homepage": "https://github.com/il-sommo/taskjuggler-vscode#readme",
  "keywords": [
    "taskjuggler",
    "project management",
    "syntax highlighting",
    "snippets",
    "code completion",
    "tjp",
    "tji",
    "project planning",
    "resource management"
  ],
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@types/vscode": "^1.108.1",
    "@vscode/vsce": "^3.7.1",
    "typescript": "^5.9.3"
  }
}
