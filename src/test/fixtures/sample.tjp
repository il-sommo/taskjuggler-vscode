project test "Test Project" 2024-01-01 +6m {
    timezone "UTC"
    currency "USD"
}

resource dev1 "Developer 1" {
    rate 500.0
    efficiency 1.0
}

resource dev2 "Developer 2" {
    rate 450.0
}

task project "Project Tasks" {
    task planning "Planning Phase" {
        effort 5d
        allocate dev1
    }

    task implementation "Implementation Phase" {
        depends !planning
        effort 20d
        allocate dev1, dev2
        start 2024-02-01
    }

    task testing "Testing Phase" {
        depends !implementation
        duration 10d
    }
}

taskreport overview "" {
    formats html
    columns name, start, end, effort
}
